{
  "name": "cvview",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "fetch-mock": "^5.10.0",
    "json-server": "^0.10.0",
    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-scripts": "0.9.5",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "concurrently \"npm run start:db\" \"npm run start:react\"",
    "start:react": "react-scripts start",
    "start:db": "json-server --watch db.json --host 0.0.0.0 --port 3001",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "seed": "cp -f db.sample.json db.json",
    "postinstall": "npm run seed"
  }
}
